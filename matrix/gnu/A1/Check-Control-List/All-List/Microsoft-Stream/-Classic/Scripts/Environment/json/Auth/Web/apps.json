{
    "$schema": "http://json.schemastore.org/global",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "minLength": 2,
            "maxLength": 20
        },
        "age": {
            "type": "integer",
            "minimum": 0
        },
        "address": {
            "type": "object",
            "properties": {
                "street": {
                    "type": "string",
                    "minLength": 5,
                    "maxLength": 50
                },
                "city": {
                    "type": "string",
                    "minLength": 2,
                    "maxLength": 30
                },
                "state": {
                    "type": "string",
                    "minLength": 2,
                    "maxLength": 30
                },
                "zip": {
                    "type": "string",
                    "pattern": "^\\d{5}(?:[-\\s]\\d{4})?$"
                }
            },
            "required": [
                "street",
                "city",
                "state",
                "zip"
            ]
        },
        "hobbies": {
            "type": "array",
            "items": {
                "type": "string",
                "minLength": 3,
                "maxLength": 30
            },
        
            "minItems": 2,
            "maxItems": 5,
            "uniqueItems": true,
            "patternProperties": {
                "^[a-zA-Z]+$": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "contains": {
                "type": "string",
                "enum": ["reading", "painting", "cooking"]
            },
            "dependencies": {
                "hobbies": ["address.city"]
            },
            "anyOf": [
                {
                    "properties": {
                        "address.city": {
                            "type": "string",
                            "minLength": 2,
                            "maxLength": 30
                        }
                    },
                    "required": ["address.city"]
                },
    
                {
                    "properties": {
                        "address.zip": {
                            "type": "string",
                            "pattern": "^\\d{5}(?:[-\\s]\\d{4})?$"
                        }
                    },
                    "required": ["address.zip"]
                }
            ],
            "not": {
                "properties": {
                    "address.city": {
                        "type": "string",
                        "minLength": 2,
                        "maxLength": 30
                    }
                },
                "required": ["address.city"]
            },
            "enum": [
                {
                    "name": "John Doe",
                    "age": 30,
                    "address": {
                        "street": "123 Main St",
                        "city": "New York",
                        "state": "New York",
                        "zip": "10001"
                    },
                    "hobbies": ["reading", "painting"],
                    "phone": "1234567890"
                },

        
                {
                    "name": "Jane Doe",
                    "age": 25,
                    "address": {
                        "street": "456 Elm St",
                        "city": "Los Angeles",
                        "state": "California",
                        "zip": "90001"
                    },
                    "hobbies": ["painting", "cooking"],
                    "phone": "9876543210"
                }
            ],
            "minProperties": 3,
            "maxProperties": 5,
